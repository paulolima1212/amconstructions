{
  "extends": ["next/core-web-vitals", "eslint:recommended", "next"],
  "rules": {
    "indent": "off",
    "react/no-unescaped-entities": "off",
    "@next/next/no-page-custom-font": "off",
    "no-unused-vars": [
      "error",
      { "vars": "none", "args": "after-used", "ignoreRestSiblings": false }
    ]
  }
}
